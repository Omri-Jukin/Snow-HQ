{
  "name": "snow-hq",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev:crm": "npm run dev --prefix apps/crm",
    "build:crm": "npm run build --prefix apps/crm",
    "start:crm": "npm run start --prefix apps/crm",
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "dev:studio": "npm run dev --prefix apps/studio",
    "build:studio": "npm run build --prefix apps/studio",
    "start:studio": "npm run start --prefix apps/studio",
    "test": "npm run test:int && npm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" npx playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/styled-engine-sc": "^7.3.1",
    "@mui/x-data-grid": "^8.10.0",
    "@mui/x-date-pickers": "^8.10.0",
    "@payloadcms/db-postgres": "3.50.0",
    "@payloadcms/next": "3.50.0",
    "@payloadcms/payload-cloud": "3.50.0",
    "@payloadcms/richtext-lexical": "3.50.0",
    "@payloadcms/ui": "3.50.0",
    "@react-pdf/renderer": "^4.2.2",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.54.0",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "drizzle-orm": "^0.44.4",
    "framer-motion": "^12.23.12",
    "graphql": "^16.8.1",
    "mammoth": "^1.8.0",
    "md-to-pdf": "^5.2.4",
    "next": "15.4.4",
    "next-intl": "^3.26.5",
    "payload": "3.50.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "puppeteer": "^23.11.1",
    "react": "19.1.0",
    "react-calendly": "^4.4.0",
    "react-dom": "^19.1.0",
    "sharp": "0.34.2",
    "styled-components": "^6.1.19",
    "three": "^0.179.1",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@playwright/test": "1.54.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "prettier": "^3.4.2",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "imports": {
    "@*": "./src/*",
    "#*": "./Components/*",
    "~*": "./*"
  }
}
